	</servlet>
	<servlet-mapping>
		<servlet-name>editMemberAmount</servlet-name>
		<url-pattern>/editMemberAmount</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>deleteMember</servlet-name>
