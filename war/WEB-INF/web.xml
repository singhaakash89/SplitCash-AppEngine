	</servlet>
	<servlet-mapping>
		<servlet-name>deleteMember</servlet-name>
		<url-pattern>/deleteMember</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>sendChat</servlet-name>
		<servlet-class>com.app.server.sendChat</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>sendChat</servlet-name>
		<url-pattern>/sendChat</url-pattern>
	</servlet-mapping>


</web-app>